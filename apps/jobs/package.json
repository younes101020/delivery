{
  "name": "@delivery/jobs",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "exports": {
    "./types": "./src/db/schema.ts",
    "./http": "./src/app.ts",
    "./hc": {
      "import": {
        "types": "./dist/src/hc.d.ts",
        "default": "./dist/src/hc.js"
      }
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node ./dist/src/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "db:migrate": "drizzle-kit migrate",
    "db:reset": "npx tsx src/db/reset.ts",
    "db:seed": "npx tsx src/db/seed.ts"
  },
  "dependencies": {
    "@bull-board/api": "^6.9.6",
    "@bull-board/hono": "^6.9.6",
    "@electric-sql/pglite": "^0.2.13",
    "@hono/node-server": "^1.13.2",
    "@hono/zod-openapi": "^0.16.4",
    "@octokit/auth-app": "^7.1.3",
    "@scalar/hono-api-reference": "^0.5.156",
    "bullmq": "^5.41.5",
    "dockerode": "^4.0.4",
    "drizzle-kit": "^0.28.1",
    "drizzle-orm": "0.36.4",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.7.8",
    "hono-pino": "^0.4.0",
    "ioredis": "^5.4.1",
    "minio": "^8.0.5",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "postgres": "^3.4.5",
    "puppeteer": "^24.0.0",
    "sharp": "^0.33.5",
    "ssh2": "^1.16.0",
    "stoker": "^1.2.7",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@delivery/eslint-config": "workspace:*",
    "@faker-js/faker": "^9.3.0",
    "@types/dockerode": "^3.3.34",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^22.15.19",
    "@types/pg": "^8.11.10",
    "@types/ssh2": "^1.15.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
