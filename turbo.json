{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {},
    "lint": {},
    "migrate": {
      "outputs": [],
      "env": ["LOG_LEVEL", "DATABASE_URL", "BEARER_TOKEN"]
    }
  }
}
